    "@types/jest": "^29.2.4",
    "country-flag-icons": "^1.5.5",
    "country-list": "^2.3.0",
    "country-to-iso": "^1.3.0",
    "countryjs": "^1.8.0",
    "dotenv": "^16.0.3",
    "expo": "^47.0.0",
    "expo-status-bar": "~1.4.2",
    "firebase": "^9.6.11",
